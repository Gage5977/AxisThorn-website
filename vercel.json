{
  "version": 2,
  "builds": [
    {
      "src": "invoices/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "invoices/public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/invoices/api/(.*)",
      "dest": "/invoices/server.js"
    },
    {
      "src": "/invoices/(.*)",
      "dest": "/invoices/public/$1"
    },
    {
      "src": "/invoices",
      "dest": "/invoices/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/www.axisthorn.com",
      "destination": "https://axisthorn.com",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}