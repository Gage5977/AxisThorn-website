{
  "rewrites": [
    {
      "source": "/invoices/:path*",
      "destination": "/invoices/public/:path*"
    },
    {
      "source": "/invoices",
      "destination": "/invoices/public/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/www.axisthorn.com",
      "destination": "https://axisthorn.com",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}