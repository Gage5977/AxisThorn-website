{
  "public": "public",
  "rewrites": [
    {
      "source": "/invoices",
      "destination": "/invoices.html"
    }
  ],
  "redirects": [
    {
      "source": "/www.axisthorn.com/(.*)",
      "destination": "https://axisthorn.com/$1",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}